<h3>All Collaborators</h3>
<hr>
<div class="row">
<% @users.each do |user| %>
  <div class="col-md-3">
    <div class="well text-center">
      <p class="thumbnail"><%= image_tag "blur-sm.jpg" %></p>
      <p><i class="fa fa-question-circle fa-5x text-muted"></i></p>
      <h4><%= user.email %></h4>
      <p><%= user.role %></p>TODO PUBLIC USER PROFILE
      <%= link_to 'user profile', user_path(user), class: 'btn btn-secondary' %>
    </div> <!-- /.well -->
  </div>
<% end %>
</div>